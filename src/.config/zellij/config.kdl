            player_1 96 165 250      // blue-400
            player_2 74 222 128      // green-400
            player_3 251 191 36      // amber-400
            player_4 248 113 113     // red-400
            player_5 244 114 182     // pink-400
            player_6 34 211 238      // cyan-400
            player_7 251 146 60      // orange-400
            player_8 167 139 250     // violet-400
            player_9 203 213 225     // slate-300
            player_10 100 116 139    // slate-500
        }
    }

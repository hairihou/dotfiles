            emphasis_2 251 191 36    // amber-400
            emphasis_3 248 113 113   // red-400
        }
        table_cell_selected {
            base 241 245 249         // slate-100
            background 51 65 85      // slate-700
            emphasis_0 96 165 250    // blue-400

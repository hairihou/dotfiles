// https://tailwindcss.com/docs/colors
// Tailwind CSS Dark theme (new format)
themes {
    tailwindcss-dark {
        text_unselected {
            base 226 232 240         // slate-200
            background 15 23 42      // slate-900
            emphasis_0 96 165 250    // blue-400 (info)
            emphasis_1 74 222 128    // green-400 (success)
            emphasis_2 251 191 36    // amber-400 (warning)
            emphasis_3 248 113 113   // red-400 (error)
        }
        text_selected {
            base 241 245 249         // slate-100
            background 51 65 85      // slate-700
            emphasis_0 96 165 250    // blue-400
            emphasis_1 74 222 128    // green-400
            emphasis_2 251 191 36    // amber-400
            emphasis_3 248 113 113   // red-400
        }
        ribbon_unselected {
            base 148 163 184         // slate-400
            background 30 41 59      // slate-800
            emphasis_0 96 165 250    // blue-400
            emphasis_1 74 222 128    // green-400
            emphasis_2 251 191 36    // amber-400
            emphasis_3 248 113 113   // red-400
        }
        ribbon_selected {
            base 241 245 249         // slate-100
            background 51 65 85      // slate-700
            emphasis_0 96 165 250    // blue-400
            emphasis_1 74 222 128    // green-400
            emphasis_2 251 191 36    // amber-400
